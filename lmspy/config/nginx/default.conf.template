server {
    listen 0.0.0.0:80;
    
    location = /favicon.ico {
        log_not_found off;
        access_log off;
    }
    
    location / {
        proxy_pass http://web:8000/;
    }

    location /static/ {
        alias /code/static/;
    }

    location /media/ {
        alias /code/media/;
    }
}
