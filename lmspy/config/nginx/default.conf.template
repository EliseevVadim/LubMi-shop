server {
    listen 80 default_server;
    
    location = /favicon.ico {
        log_not_found off;
        access_log off;
    }
    
    location / {
        proxy_pass http://web:8000/;
    }

    location /static/ {
        root /code/;
    }

    location /media/ {
        root /code/;
    }
}
